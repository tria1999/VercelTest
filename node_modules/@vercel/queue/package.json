{
  "name": "@vercel/queue",
  "version": "0.0.0-alpha.33",
  "publishConfig": {
    "access": "public"
  },
  "description": "A Node.js library for interacting with the Vercel Queue Service API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "vercel-queue-local-init": "./bin/local-discover.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./nextjs/pages": {
      "import": {
        "types": "./dist/nextjs-pages.d.mts",
        "default": "./dist/nextjs-pages.mjs"
      },
      "require": {
        "types": "./dist/nextjs-pages.d.ts",
        "default": "./dist/nextjs-pages.js"
      }
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "vercel",
    "queue",
    "typescript",
    "api",
    "client"
  ],
  "author": "Timer <timer@vercel.com>",
  "license": "MIT",
  "dependencies": {
    "@vercel/oidc": "^3.0.5",
    "mixpart": "0.0.5-alpha.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.8",
    "@types/node": "^22.15.24",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/ui": "^3.1.4",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^3.1.4"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  }
}